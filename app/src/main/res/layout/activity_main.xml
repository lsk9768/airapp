                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/seoul"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="서울" />

                <ProgressBar
                    android:id="@+id/seoulBar"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:max="100"
                    android:progress="0" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/busan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="부산" />

                <ProgressBar
                    android:id="@+id/busanBar"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:max="100"
                    android:progress="0" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/daegu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="대구" />

                <ProgressBar
                    android:id="@+id/daeguBar"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:max="100"
                    android:progress="0" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/incheon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="인천" />

                <ProgressBar
                    android:id="@+id/incheonBar"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:max="100"
                    android:progress="0" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/gwangju"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="광주" />

